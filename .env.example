# Local development only. Do not commit real keys.
GOOGLE_MAPS_ANDROID_API_KEY=your_real_key_here

# Optional fallback (not needed if you set GOOGLE_MAPS_ANDROID_API_KEY)
# EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_real_key_here

# Used by the JS runtime to call Google Directions + Places Autocomplete.
# Enable both "Directions API" and "Places API" in Google Cloud Console.
# For production, prefer calling these from your backend.
EXPO_PUBLIC_GOOGLE_DIRECTIONS_API_KEY=your_real_key_here
